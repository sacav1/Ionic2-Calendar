{
  "name": "ionic2-calendar",
  "version": "0.2.14",
  "description": "Ionic2 calendar component",
  "keywords": [
    "Ionic2",
    "calendar"
  ],
  "author": {
    "name": "twinsbc"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twinssbc/Ionic2-Calendar.git"
  },
  "scripts": {
    "build": "rm -rf dist",
    "build-prod": "rm -rf aot && node_modules/.bin/ngc -p tsconfig-aot.json && node_modules/.bin/ngc src && mv src/*ngfactory.ts aot && rm -r aot/waste",
    "copy_static_files": "cp -r package.json README.md LICENSE tsconfig.json dist/",
    "copy_static_files_prod": "cp -r package.json README.md LICENSE tsconfig.json aot/",
    "dev": "tsc --watch",
    "build-debug": "npm run build-prod && npm run copy_static_files_prod && cd aot && npm pack"
  },
  "main": "./index.js",
  "dependencies": {},
  "devDependencies": {
    "@angular/animations": "4.4.3",
    "@angular/common": "4.4.3",
    "@angular/compiler": "4.4.3",
    "@angular/compiler-cli": "4.4.3",
    "@angular/core": "4.4.3",
    "@angular/forms": "4.4.3",
    "@angular/http": "4.4.3",
    "@angular/platform-browser": "4.4.3",
    "@angular/platform-browser-dynamic": "4.4.3",
    "@angular/platform-server": "4.4.3",
    "@types/es6-shim": "^0.31.35",
    "intl": "1.2.5",
    "ionic-angular": "3.7.1",
    "rxjs": "5.4.3",
    "tslint-ionic-rules": "0.0.11",
    "typescript": "2.5.2",
    "zone.js": "0.8.18"
  }
}
